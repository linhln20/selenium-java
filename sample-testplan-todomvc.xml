<suite name="TodoMVC">
    <groups>
        <run>
            <exclude name="wip"/>
        </run>
    </groups>
        <test name="Chrome Browser headless">
            <parameter name="browser" value="chrome-headless"/>
            <parameter name="url" value="https://todomvc.com/examples/react/dist/"/>
            <classes>
                <class name="todomvc.TodoMVCTest"/>
            </classes>
        </test>
</suite>